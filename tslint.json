{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "no-console": [false],
    "object-literal-sort-keys": [false],
    "no-trailing-whitespace": [false],
    "quotemark": [true, "single"],
    "semicolon": false,
    "ordered-imports": false,
    "arrow-parens": false,
    "curly": false
  },
  "rulesDirectory": []
}