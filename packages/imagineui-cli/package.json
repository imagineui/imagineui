{
  "name": "imagineui-cli",
  "version": "0.2.0",
  "description": "CLI tool to generate mock-ups from a localized human-readable format",
  "scripts": {
    "start": "webpack; webpack --config ./webpack.renderer.config.js; node ./build/bundle.js",
    "start:built": "node ./build/bundle.js",
    "dev": "npm-run-all --parallel watch:server watch:build",
    "watch:server": "nodemon \"./build/bundle.js\" --watch \"./build\" ",
    "watch:build": "webpack --watch",
    "build": "webpack; webpack --config ./webpack.renderer.config.js",
    "startOld": "ts-node src/app.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "vadkou",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/commander": "^2.12.2",
    "@types/node": "^12.11.2",
    "@types/puppeteer": "^2.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "3.7.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "preact-render-to-string": "^5.1.3",
    "puppeteer": "^2.0.0",
    "ts-node": "^7.0.1"
  }
}
